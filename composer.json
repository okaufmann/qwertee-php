{
    "name": "okaufmann/qwertee-php",
    "description": "Simple client to access Qwertees (https://www.qwertee.com) todays and last chance tees",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Kaufmann",
            "email": "okaufmann91@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "simplepie/simplepie": "^1.4",
        "illuminate/support": "5.*|^6.0",
        "symfony/dom-crawler": "^5.0",
        "symfony/css-selector": "^5.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Okaufmann\\Qwertee\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okaufmann\\Tests\\Qwertee\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
