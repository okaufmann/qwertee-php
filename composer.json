{
    "name": "okaufmann/qwertee-php",
    "description": "Simple client to access Qwertees (https://www.qwertee.com) todays and last chance tees",
    "keywords": [
        "okaufmann",
        "qwertee-php"
    ],
    "homepage": "https://github.com/okaufmann/qwertee-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Oliver Kaufmann",
            "email": "okaufmann91@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*|^6.0",
        "simplepie/simplepie": "^1.5.3",
        "symfony/css-selector": "^5.0",
        "symfony/dom-crawler": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Okaufmann\\QwerteePhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okaufmann\\QwerteePhp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
