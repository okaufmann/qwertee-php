{
    "name": "okaufmann/qwertee-php",
    "description": "Simple client to access Qwertees (https://www.qwertee.com) todays and last chance tees",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Kaufmann",
            "email": "okaufmann91@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "simplepie/simplepie": "^1.4",
        "illuminate/support": "5.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Okaufmann\\Qwertee\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okaufmann\\Tests\\Qwertee\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
